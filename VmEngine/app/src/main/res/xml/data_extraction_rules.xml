<?xml version="1.0" encoding="utf-8"?>
<!--
  Android 12+ 数据提取规则：
  - cloud-backup: 云备份范围；
  - device-transfer: 设备迁移范围。
  当前保持模板占位，未启用具体路径。
-->
<data-extraction-rules>
    <!-- 云备份规则。 -->
    <cloud-backup>
        <!--
          按需启用：
          <include .../>
          <exclude .../>
        -->
    </cloud-backup>
    <!-- 设备迁移规则（当前未启用）。 -->
    <!--
    <device-transfer>
        <include .../>
        <exclude .../>
    </device-transfer>
    -->
</data-extraction-rules>
